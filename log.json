RowDataPacket {
  Id: 110652984,
  url: 'https://api.github.com/repos/AzureAD/azure-activedirectory-identitymodel-extensions-for-dotnet/issues/277',
  repository_url: 'https://api.github.com/repos/AzureAD/azure-activedirectory-identitymodel-extensions-for-dotnet',
